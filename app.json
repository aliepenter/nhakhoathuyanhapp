{
  "expo": {
    "name": "Nha khoa Thuy Anh",
    "slug": "nha-khoa-thuy-anh",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "nha-khoa-thuy-anh",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splashscreen.jpg",
      "resizeMode": "cover"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "image": "./assets/images/splashscreenandroid.png",
        "resizeMode": "cover"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}